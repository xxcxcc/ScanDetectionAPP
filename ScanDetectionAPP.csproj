<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resource\detect.png" />
    <None Remove="Resource\密码.png" />
    <None Remove="Resource\激光雷达.ico" />
    <None Remove="Resource\激光雷达.png" />
    <None Remove="Resource\用户.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="HandyControl" Version="3.5.1" />
    <PackageReference Include="log4net" Version="3.2.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resource\detect.png" />
    <Resource Include="Resource\密码.png" />
    <Resource Include="Resource\激光雷达.ico" />
    <Resource Include="Resource\激光雷达.png" />
    <Resource Include="Resource\用户.png" />
  </ItemGroup>

  <ItemGroup>
    <None Update="log4net.config">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>